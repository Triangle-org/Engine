{
    "name": "localzet/framex",
    "description": "FrameX (FX) Engine - Ядро фреймворка нового поколения. Является частью проекта Localzet Developmet Kit",
    "type": "library",
    "keywords": [
        "localzet",
        "library",
        "core",
        "framex"
    ],
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "Ivan Zorin",
            "email": "creator@localzet.ru",
            "homepage": "https://vk.com/localzet",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@localzet.ru",
        "docs": "https://localzet.gitbook.io"
    },
    "funding": [
        {
            "type": "other",
            "url": "https://www.tinkoff.ru/cf/Ad5MBNhodod"
        }
    ],
    "suggest": {
        "ext-event": "For better performance. ",
        "jenssegers/mongodb-session": "Add MongoDB session support to Laravel-MongoDB",
        "jenssegers/mongodb-sentry": "Add Sentry support to Laravel-MongoDB"
    },
    "require": {
        "php": ">=8.1",
        "ext-json": "*",
        "ext-curl": "*",
        "ext-mysqli": "*",
        "localzet/core": ">=1.2.3",
        "psr/container": "^1.1.1",
        "nikic/fast-route": "^1.3",
        "monolog/monolog": "^2.0",
        "jenssegers/blade": "^1.4.0",
        "topthink/think-template": "^2.0.8",
        "twig/twig": "^3.3.10",
        "illuminate/database": "^8.0",
        "illuminate/pagination": "*",
        "illuminate/events": "^8.0",
        "symfony/var-dumper": "^6.1",
        "php-di/php-di": "^6.4",
        "doctrine/annotations": "^1.13",
        
        "illuminate/support": "^8.0",
        "illuminate/container": "^8.0",
        "mongodb/mongodb": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "localzet\\FrameX\\": "src/",
            "support\\": "src/support/",
            "Jenssegers\\Mongodb\\": "src/support/mongodb/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jenssegers\\Mongodb\\MongodbServiceProvider",
                "Jenssegers\\Mongodb\\MongodbQueueServiceProvider"
            ]
        }
    }
}